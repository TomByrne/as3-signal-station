package org.farmcode.display.constants
{
	public class Scale
	{
		/** Instruction to never scale the display */
		public static const NEVER:String = "scaleNever";
		/** Instruction to always scale the display */
		public static const ALWAYS:String = "scaleAlways";
		/** Instruction to only scale up the display */
		public static const UP_ONLY:String = "scaleUpOnly";
		/** Instruction to only scale down the display */
		public static const DOWN_ONLY:String = "scaleDownOnly";
	}
}