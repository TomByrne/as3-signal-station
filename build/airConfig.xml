<?xml version="1.0"?>

<flex-config>
	 <compiler>
		<keep-as3-metadata>
			<name>ActRule</name>
			<name>ActReaction</name>
		</keep-as3-metadata>
		
		<define append="true">
        	<name>CONFIG::debug</name>
        	<value>false</value>
    	</define>
		
		<define append="true">
        	<name>PLATFORM::web</name>
        	<value>false</value>
    	</define>
		
		<define append="true">
        	<name>PLATFORM::air</name>
        	<value>true</value>
    	</define>
		
		<optimize>true</optimize>
		<warn-assignment-within-conditional>false</warn-assignment-within-conditional>
		<warn-no-constructor>false</warn-no-constructor>
	 </compiler>
	 
	 <metadata>
		<publisher>tbyrne.org</publisher>
		<creator>Thomas Byrne</creator>
		<language>EN</language>
	 </metadata>
</flex-config>
